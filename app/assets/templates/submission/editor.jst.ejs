<div id='editor-main'>
  <div id='editor'></div>

  <form id='editor-form' role='form' action='#'>
    <textarea id='caption' name='caption' placeholder='Write a caption...' 
      rows='2'></textarea>
    <button id='upload' class='btn btn-default'>Upload</button>
    <button id='cancel' class='btn btn-default pull-right'>Cancel</button>
  </form>
</div>

<div id='editor-controls'>
  <ul class='nav nav-tabs' role='tablist'>
    <li class='active'>
      <a href='#filters' role='tab' data-toggle='tab'>Filters</a>
    </li>
    <li>
      <a href='#adjustments' role='tab' data-toggle='tab'>Adjustments</a>
    </li>
  </ul>

  <div class='tab-content'>
    <div id='filters' class='tab-pane active'>
      <div data-name='none' class='filter-preview filter'>
        <span>None</span>
        <img src="https://s3-us-west-1.amazonaws.com/any-gram-prod/images/resources/filter-previews/none.png" alt="Filter - None" />
      </div>
      <div data-name='claribel' class="filter-preview filter">
        <span>Claribel</span>
        <img src="https://s3-us-west-1.amazonaws.com/any-gram-prod/images/resources/filter-previews/claribel.png" alt="Filter - Claribel" />
